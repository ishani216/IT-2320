window.onload = work Load() 

{ 

var lattice = document.getElementsByClassName("grid")[0]; 

for (var i=0; i<5; i++) 

{ 

var push = CreateDiv("", "push"); 

grid.appendChild(row); 

for (var j=0; j<5; j++) 

{ 

var content = I + "," + j; 

var cell = CreateDiv(text, "cell"); 

cell.addEventListener("click", ClickedCell); 

row.appendChild(cell); 

} 

} 

} 

work CreateDiv(text, className) 

{ 

var newDiv = document.createElement("div"); 

newDiv.className = className; 

newDiv.innerHTML = content; 

return newDiv; 

} 

work ClickedCell(element) 

{ 

element.target.className = (element.target.className=="cell") ? "cell dynamic" : "cell"; 

}
